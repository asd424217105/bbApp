.container.blue .achienement-box .title-box{font-size:32rpx;display: flex;height: 40rpx;line-height: 40rpx;overflow: hidden;}
.container.blue .achienement-box .title-box .title{width: 502rpx;font-weight: 600;overflow: hidden;text-overflow:ellipsis;}
.container.blue .achienement-box .title-box button::after{border: none;background: none;padding:0;margin:0;}
.container.blue .achienement-box .title-box button{background: none;padding:0;margin:0;font-size: 32rpx;color: #169CD9;line-height: 40rpx;margin-left: 40rpx;}
.container.blue .achienement-box .time-workType-box{height: 40rpx;line-height: 40rpx;display: flex;font-size: 32rpx;overflow: hidden;margin:20rpx 0;}
.container.blue .achienement-box .time-workType-box .time{width: 400rpx;}
.container.blue .achienement-box .time-workType-box .time span{margin:0 10rpx;}
.container.blue .achienement-box .time-workType-box .time-workType{width: 300rpx;margin-left: 40rpx;margin-right: 16rpx;text-align: right;}
.container.blue .achienement-box .content{font-size: 32rpx;margin-bottom: 20rpx;}

