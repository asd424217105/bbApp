/* 弹窗 */
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 900;
  color: #fff;
}
.modal-dialog {
  width: 640rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 999;
  background: #f9f9f9;
  margin: -194rpx 55rpx;
  border-radius: 10rpx;
}
.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}
.modal-content {
  padding: 50rpx 32rpx 10rpx;
  font-size: 28rpx;
}
.modal-item{
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 60rpx;
  margin-bottom: 20rpx;
}
.modal-item text{
  width: 140rpx;
}
.modal-item input {
  width: 280rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  padding: 0 10rpx;
  border: 2rpx solid #ccc;
  margin-right: 20rpx;
  box-sizing: border-box;
  color: #333;
}
.modal-item picker{
  width: 280rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  padding: 0 10rpx;
  border: 2rpx solid #ccc;
  margin-right: 20rpx;
  box-sizing: border-box;
  color: #333;
}
input-holder {
  color: #666;
  font-size: 28rpx;
}
.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}
.modal-footer button{
  margin: 0;
  border-radius: none;
  background: #fff;
  border: none;
  text-align: center;
  width: 50%;
}
.btn-cancel {
  color: #666;
}
.btn-confirm {
  color: #1c97e5;
}

