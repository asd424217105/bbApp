/* pages/index/pages/contractList/addContract/addContract.wxss */
@import "/pages/common/lib/upload.wxss";   
.page-section-spacing{
  padding: 10rpx 20rpx;
}
/* 单按钮提交 */
.one-btn{
  position: fixed;
  bottom: 20rpx;
  width: 100%;
  box-sizing: border-box;
}
.one-btn button{
  width: 90%;
  color: #fff;
  font-size: 30rpx;
  background-color: #e51c23;
  border-radius: 10rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.one-btn button.button-hover{
  background-color: gainsboro;
}